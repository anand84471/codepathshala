@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="google-signin-client_id" content="186541045153-5jotabtj8m629ff03re17cv6kq2vlsri.apps.googleusercontent.com">
    @if (ViewBag.Title != null)
    {
        <title>@ViewBag.Title</title>
    }
    else
    {
        <title>ReadMyCourse : Where knowledge has no boundaries.</title>
    }
    @if (ViewBag.Description != null)
    {
        <meta name="description" content="@ViewBag.Description">
    }
    else
    {
        <meta name="description" content="Learn Python, Data Science, MATLAB and Various free courses. Learn live from top instructors.">
    }

    <link href="https://fonts.googleapis.com/css?family=Work+Sans:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Fredericka+the+Great" rel="stylesheet">
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.css" rel="stylesheet">

    <link rel="stylesheet" href="../../Style/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="../../Style/animate.css">
    <script src="../../Scripts/vendor/jquery/jquery.min.js"></script>
    <link rel="stylesheet" href="../../Style/ionicons.min.css">
    <link rel="stylesheet" href="../../Style/flaticon.css">
    <link rel="stylesheet" href="../../Style/icomoon.css">
    <link rel="stylesheet" href="../../Style/style.css">
    <link rel="stylesheet" href="../../Style/owl.carousel.min.css">
</head>

<body>
    @{Html.RenderPartial("_MenuAbout");}
    @RenderBody()
    <footer class="ftco-footer ftco-bg-dark ftco-section">
        <div class="container">
            <div class="row mb-5">
                <div class="col-lg-3 col-md-6 ">
                    <div class="ftco-footer-widget mb-5">
                        <h2 class="ftco-heading-2">Have a Questions?</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li><span class="icon icon-map-marker"></span><span class="text">Noida 75, UP, India</span></li>
                                <li><a href="#"><span class="icon icon-phone"></span><span class="text">+91 7317553431</span></a></li>
                                <li><a href="#"><span class="icon icon-envelope"></span><span class="text">info@readmycourse.com</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>

                
                <div class="col-lg-3 col-md-6 ">
                    <div class="ftco-footer-widget mb-5">
                        <h2 class="ftco-heading-2 mb-0">Download App</h2>
                        <ul class="list-unstyled float-md-left float-lft mt-3">
                            <li class="ftco-animate"><a href="https://play.google.com/store/apps/details?id=com.readmycourse.android" target="_blank"><img class="img" style="max-width:200px;max-height:70px;" src="~/Images/google-play-logo.png"/></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3">
                    <div class="ftco-footer-widget mb-5 ml-md-4">
                        <h2 class="ftco-heading-2">Links</h2>
                        <ul class="list-unstyled">
                            <li><a href="~/"><span class="ion-ios-arrow-round-forward mr-2"></span>Home</a></li>
                            <li><a href="~/Classroom"><span class="ion-ios-arrow-round-forward mr-2"></span>Classroom</a></li>
                            <li><a href="~/Course"><span class="ion-ios-arrow-round-forward mr-2"></span>Courses</a></li>
                            <li><a href="~/Test"><span class="ion-ios-arrow-round-forward mr-2"></span>Free Tests</a></li>
                            <li><a href="~/AboutUs"><span class="ion-ios-arrow-round-forward mr-2"></span>About Us</a></li>
                            <li><a href="~/Student"><span class="ion-ios-arrow-round-forward mr-2"></span>Student Login</a></li>
                            <li><a href="~/Instructor"><span class="ion-ios-arrow-round-forward mr-2"></span>Instructor Login</a></li>
                            <li><a href="~/TermsAndConditions"><span class="ion-ios-arrow-round-forward mr-2"></span>Terms and Conditions</a></li>
                            <li><a href="~/Privacy"><span class="ion-ios-arrow-round-forward mr-2"></span>Privacy policy</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6 col-lg-3">
                    <div class="ftco-footer-widget mb-5">
                        <h2 class="ftco-heading-2">Subscribe Us!</h2>
                        <form action="#" class="subscribe-form">
                            <div class="form-group">
                                <input type="text" id="subscriptionEmail"  class="form-control mb-2 text-center" placeholder="Enter email address">
                                <input type="submit" onclick="subscribe()" value="Subscribe" class="form-control submit px-3">
                            </div>
                        </form>
                    </div>
                    <div class="ftco-footer-widget mb-5 ">
                        <h2 class="ftco-heading-2 mb-0">Connect With Us</h2>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-3">
                            <li class="ftco-animate"><a href="https://www.facebook.com/readmycourseofficial" target="_blank"><span class="icon-facebook"></span></a></li>
                            <li class="ftco-animate"><a href="https://www.youtube.com/channel/UC7VJXgLUMgDICMmsCwhnmlQ/" target="_blank"><span class="icon-youtube"></span></a></li>
                            <li class="ftco-animate"><a href="https://www.instagram.com/readmycourse/" target="_blank"><span class="icon-instagram"></span></a></li>
                            <li class="ftco-animate"><a href="https://play.google.com/store/apps/details?id=com.readmycourse.android" target="_blank"><span class="icon-android"></span></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>
                        Copyright &copy;
                        <script>document.write(new Date().getFullYear());</script> All rights reserved <a href="https://www.readmycourse.com"> | ReadMyCourse</a> 
                    </p>
                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        This template is made with <i class="icon-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>
        </div>
        <div class="modal fade" id="subscriptionSuccessModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title text-success" id="exampleModalLabel">Success</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="modal-body">
                            <h4>Thank you for subscrbing us.</h4>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    <script src="../../Scripts/AboutScripts/jquery.min.js"></script>
    <script src="../../Scripts/AboutScripts/jquery-migrate-3.0.1.min.js"></script>
    <script src="../../Scripts/AboutScripts/popper.min.js"></script>
    <script src="../../Scripts/AboutScripts/bootstrap.min.js"></script>
    <script src="../../Scripts/AboutScripts/jquery.easing.1.3.js"></script>
    <script src="../../Scripts/AboutScripts/jquery.waypoints.min.js"></script>
    <script src="../../Scripts/AboutScripts/jquery.stellar.min.js"></script>
    <script src="../../Scripts/AboutScripts/owl.carousel.min.js"></script>
    <script src="../../Scripts/AboutScripts/jquery.magnific-popup.min.js"></script>
    <script src="../../Scripts/AboutScripts/aos.js"></script>
    <script src="../../Scripts/AboutScripts/jquery.animateNumber.min.js"></script>
    <script src="../../Scripts/AboutScripts/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="../../Scripts/AboutScripts/google-map.js"></script>
    <script src="../../Scripts/AboutScripts/main.js"></script>
    <script type="text/javascript">
        function subscribe() {
            var _data = {
                "email_address": $("#subscriptionEmail").val()
            };
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                url: "/api/v1/document/Subscribe",
                data: JSON.stringify(_data),
                success: function (data) {
                    debugger;
                    if (data != null && data.response_code == 1) {
                        $("#subscriptionSuccessModal").modal("show");
                    }
                }
            });
        }

    </script>
</body>
</html>

