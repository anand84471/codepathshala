    function requestUploadProfile() {
        document.getElementById("ProfileUploadProgressBar").style.width = "0%";
        debugger
        var data = new FormData();
        var file = $('#fileUploadProfilePicture')[0];
        data.append('file', file.files[0]);
        $.ajax({
            xhr: function () {
                var xhr = new window.XMLHttpRequest();

                xhr.upload.addEventListener("progress", function (evt) {
                    if (evt.lengthComputable) {
                        var percentComplete = evt.loaded / evt.total;
                        percentComplete = parseInt(percentComplete * 100);
                        document.getElementById("ProfileUploadProgressBar").style.width = percentComplete + "%";
                        if (percentComplete === 100) {


                        }
                    }
                }, false);

                return xhr;
            },
            headers: { "Authorization": 'Bearer ' + localStorage.getItem('jwttoken') },
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            processData: false,
            contentType: false,
            data: data,
            url: "/api/v1/Course/UploadImage",
            success: function (data) {
                debugger
                if (data != null && data.response_code == 1) {
                    profilePictureUrl = data.file_location;
                    $("#fileUploadUrl").val(data.file_location);
                    $("#mediumSizeImageUrl").val(data.medium_size_icon_url);
                    $("#smallSizeImageUrl").val(data.small_size_icon_url);
                    //profilePictureUploadSubmitButton
                    $("#profilePictureUploadSubmitButton").prop("disabled", false);
                    $("#postProfileUploadAlert").show();
                }
                else {

                }
            }
        });
    }
    var profilePictureUrl = "";
   
