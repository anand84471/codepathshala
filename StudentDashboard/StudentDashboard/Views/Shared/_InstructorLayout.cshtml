    function getAllAlerts() {
        debugger;

        $.ajax({
            headers: { "Authorization": 'Bearer ' + localStorage.getItem('access_token') },
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            url: "/api/v1/instructor/GetAllAlert",
            success: function (data) {
                debugger;
                if (data != null && data.response_code == 1) {
                    var alertHtml = '';
                    var alerts=data.alerts;
                    for(var i=0;i<alerts.length;i++)
                    {
                        alertHtml += '  <a class="dropdown-item d-flex align-items-center" href="#">'
                  +'<div class="mr-3">'
                    +'<div class="icon-circle bg-primary">'
                      + '<i class="' + alerts[i].alert_icon + ' text-white"></i>'
                    +'</div>'
                  +'</div>'
                  +'<div>'
                    + '<div class="small text-gray-500">' + alerts[i].alert_time + '</div>'
                    + '<span class="font-weight-bold">' + alerts[i].alert_message + '</span>'
                  +'</div>'
                +'</a>';
                    }
                  
                    $("#alertContainer").html(alertHtml);
                    if (alerts.length == 0) {
                        $("#alertContainer").html('<a class="dropdown-item text-center small text-gray-500" href="#">No alerts</a>');
                    }
                }
                else {
                    $("#alertContainer").html('<a class="dropdown-item text-center small text-gray-500" href="#">No alerts</a>');
                }

            }
    });
    }
