<div class="modal fade top" id="profilePictureUploadModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modal-full-height modal-top" role="document">
        <!--Content-->
        <div class="modal-content">
            <!--Header-->
            <div class="modal-header ">
                <h4 class="modal-title w-100 " id="myModalLabel"> <b>Upload New Profile Picture</b></h4>
            </div>
            <!--Body-->
            <div class="modal-body">
                <form class="card-img-top classroom-background" action="../Instructor/UpdateProfilePictute" method="post" enctype="multipart/form-data">
                    @Html.AntiForgeryToken()
                    <i class="fa fa-upload upload-icon" aria-hidden="true"></i>
                    <div class="form-group">
                        <label for="exampleFormControlFile1">Upload New Image</label>
                        <input type="file" id="fileUploadProfilePicture" name="file" accept="image/x-png,image/gif,image/jpeg" />
                        <input type="url" id="fileUploadUrl" hidden name="url" />
                        <input type="url" id="mediumSizeImageUrl" hidden name="medium_size_url" />
                        <input type="url" id="smallSizeImageUrl" hidden name="small_size_url" />
                        <button type="button" class="btn btn-primary" onclick="requestUploadProfile()">Upload</button>
                        <div class="progress progress-sm mr-2">
                            <div class="progress-bar bg-info" role="progressbar" style="width: 0%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" id="ProfileUploadProgressBar"></div>
                        </div>
                        <div class="alert alert-success" id="postProfileUploadAlert" style="display:none" role="alert">
                            profile picture added successfully
                        </div>
                        <input type="submit" value="submit" class="btn btn-primary" disabled id="profilePictureUploadSubmitButton">
                    </div>
                </form>
            </div>
            <!--Footer-->
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary " data-dismiss="modal">Close</button>
            </div>
        </div>
        <!--/.Content-->
    </div>
</div>
