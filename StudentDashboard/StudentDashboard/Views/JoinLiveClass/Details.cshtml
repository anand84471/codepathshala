<div class="course-demo-card container-fluid mt-3">
    <div class="d-sm-flex align-items-center justify-content-between mb-2">
        <h1 class="h3 mb-0 text-gray-800">Join Live Class</h1>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="~/" onclick="">Home</a></li>
                <li class="breadcrumb-item"><a href="~/JoinLiveClass" onclick="">JoinLiveClass</a></li>
                <li class="breadcrumb-item active" aria-current="page">Join classroom</li>
            </ol>
        </nav>
    </div>
    <div class="row" id="live-class-info-root">
        <div class="col-lg-12 mb-4">
            <div class="card mb-4 border-bottom-success live-class-info-root">
                <div class="card-body classroom-background-container" style="background-image:url(@Model.m_strClassroomBackgroundImage)">
                    <div class="classroom-background-mask">
                        <h1 class="text-white font-weight-bold text-gray-100 " id="classroomName">@Model.m_strClassroomName</h1>

                        @if (Model.m_strClassroomCharge != "free")
                        {
                    <h4 class="text-white" id="classroomJoiningText">Price <strong>&#8377  <span class="" id="classroomJoiningFee">@Model.m_strClassroomCharge</span>  <span class="" style="text-decoration: line-through;">@Model.m_strPromotonialCharge</span>  / US&#36 @Model.m_iPriceForForeignStudents </strong> </h4>
                        }
                        else
                        {
                            <h4 class="text-white" id="classroomJoiningText">Price free  </h4>
                        }
                        @if (Model.m_bIsRegistrationClosed)
                        {
                            <p class="text-white h6 font-weight-bolder" id="classroomCreationDate">Sorry! you can't join this course. Registration has been closed for this course.</p>
                        }
                        else
                        {
                            if (Model.m_strLiveClassStartDate != null && Model.m_strLiveClassStartDate != "")
                            {
                                if (Model.m_bIsClassStarted)
                                {
                                    <p class="text-white h6 font-weight-bolder" id="classroomCreationDate">Live class is started from @Model.m_strLiveClassStartDate.</p>
                                }
                                else
                                {
                                    <p class="text-white h6 font-weight-bolder" id="classroomCreationDate">Live class will start from @Model.m_strLiveClassStartDate.</p>
                                }
                            }
                            else
                            {

                            }
                            <span><button class="btn btn-outline-light font-weight-bold" onclick="LaunchDemoModal()" href="@ViewBag.ReturnUrl">Join free now</button></span>
                            <span class="badge badge-pill badge-light" id="successbadge" style="display:none">Joined Successfully</span>
                            if (Model.m_strClassroomCharge != "free")
                            {
                                <h5 class="text-danger font-weight-bold">Attend @Model.m_iNoOfDemoClassrooms sessions free and pay only after you are satisfied.</h5>
                            }
                            <h6 class="text-warning font-weight-bold" href="@ViewBag.ReturnUrl">Hurry! Few seats available</h6>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="course-description">
        <h2 class="font-weight-bold">About Course</h2>
        <h4 class="text-gray-300">@Model.m_strClassroomDescription</h4>
    </div>
    <div class="modal fade" id="failModal" tabindex="-1" role="dialog" aria-labelledby="modalIndexCreatedTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="card border-danger modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalIndexCreatedTitle">Error</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body text-danger">
                    <h3 id="infoMessageFail">Classroom could not be joined</h3>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
